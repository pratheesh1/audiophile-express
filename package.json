{
  "name": "ecommerce-audio-store-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build:css": "postcss ./public/styles/tailwind.css -o ./public/styles/index.css && echo '\\033[1;92m>> tailwind css build completed!'",
    "start-dev": "nodemon index.js",
    "watch": "chokidar './views/**' './forms/**' './public/styles/tailwind.css' -c \"npm run build:css\"",
    "dev": "concurrently \"npm run build:css\" \"npm run watch\" \"npm run start-dev\""
  },
  "keywords": [
    "e-commerce",
    "express",
    "node",
    "tailwind",
    "tailwindcss",
    "audio shop"
  ],
  "author": "pratheesh soman",
  "license": "ISC",
  "dependencies": {
    "bookshelf": "^1.2.0",
    "cloudinary": "^1.27.1",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "date-fns": "^2.27.0",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "forms": "^1.3.2",
    "handlebars-helpers": "^0.10.0",
    "hbs": "^4.1.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.1",
    "path": "^0.12.7",
    "session-file-store": "^1.5.0",
    "signale": "^1.4.0",
    "stripe": "^8.191.0",
    "wax-on": "^1.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.4.0",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19"
  },
  "signale": {
    "displayBadge": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true
  }
}
